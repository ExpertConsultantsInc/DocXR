  'open=>Action':{
		'callback=>Pointer':{},
    'result=>String':{value:''},
		onAction:[
      {'env.defaults.masterKeyboard.value':['concept.localId']},
      {'parent.object.visible':true},
		]
	},
  'close=>Action':{
		onAction:[
			{doIf:{value:'env.defaults.masterKeyboard.value == concept.localId',
				onTrue:[
					{'env.defaults.masterKeyboard.value':'default'},
          {'parent.object.visible':false},
				]
			}}
		]
	},

  'numString=>String':{value:''},
  'numKeyHandler=>Handler':{
    'intersect=>IntersectAction':{
      onFocused:[
        {'current.object.scalar.doAnimation':{from:1, to:1.2, fromTime:0, toTime:50}},
        {'current.buttonOutline.material.color':0xFFFFFF},
        {'current.buttonLabel.fontMtl.color':0xFFFFFF},
      ],
      onUnfocused:[
        {'current.object.scalar.doAnimation':{from:1.2, to:1, fromTime:0, toTime:50}},
        {'current.buttonOutline.material.color':0xC4F6FF},
        {'current.buttonLabel.fontMtl.color':0xC4F6FF},
      ],
      onSelected:[
        {'current.object.scalar.doAnimation':{from:1.2, to:1, fromTime:0, toTime:50}},
        {'current.object.scalar.doAnimation':{from:1, to:1.2, fromTime:50, toTime:100}},
        {'concept.numString.doAppend':['current.buttonLabel.text']},
        {'concept.numPad.result.text':['concept.numString.value']},
      ],
    }
  },

  'returnKeyHandler=>Handler':{
    'intersect=>IntersectAction':{
      onFocused:[
        {'current.object.scalar.doAnimation':{from:1, to:1.2, fromTime:0, toTime:50}},
        {'current.buttonOutline.material.color':0xFFFFFF},
        {'current.iconMesh.map':'concept.iconReturnWhiteTex'},
      ],
      onUnfocused:[
        {'current.object.scalar.doAnimation':{from:1.2, to:1, fromTime:0, toTime:50}},
        {'current.buttonOutline.material.color':0xC4F6FF},
        {'current.iconMesh.map':'concept.iconReturnBlueTex'},
      ],
      onSelected:[
        {'current.object.scalar.doAnimation':{from:1.2, to:1, fromTime:0, toTime:50}},
        {'current.object.scalar.doAnimation':{from:1, to:1.2, fromTime:50, toTime:100}},
        {'concept.open.callback.do':{'number.value':['concept.numString.value']}},
        {'concept.numPad.result.text':''},
        {'concept.numString.value':''},
//        {'concept.doUpdateNotify':{current:'current'}},
      ],
    }
  },

},
